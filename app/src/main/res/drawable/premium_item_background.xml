<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layer_list">
    <item
        android:id="@+id/item1">
        <rotate
            android:fromDegrees="-45"
            android:pivotX="0%"
            android:pivotY="100%">
